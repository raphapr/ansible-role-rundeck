# This file will be sourced before the /etc/rundeck/profile,
# allowing your exports to take precedence.

RUNDECK_TEMPDIR={{ rundeck_tmpdir }}

RDECK_JVM_SETTINGS="${RDECK_JVM_SETTINGS:- {{ rundeck_jvm_settings }} }"

RDECK_JVM_OPTS="-Drundeck.jaaslogin=true \
       -Djava.security.auth.login.config=/etc/rundeck/jaas-{{ rd_login_module }}.conf \
       -Dloginmodule.name={{ rd_login_module }}"
export PATH={{ rundeck_overrides_dir }}:$PATH
